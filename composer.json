{
    "name": "amphp/http-server",
    "homepage": "https://github.com/amphp/http-server",
    "description": "A non-blocking HTTP application server for PHP based on Amp.",
    "keywords": [
        "http",
        "server",
        "async",
        "non-blocking",
        "amp",
        "amphp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net"
        },
        {
            "name": "Bob Weinand"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
        "amphp/amp": "^2",
        "amphp/byte-stream": "^1.3",
        "amphp/hpack": "^1",
        "amphp/http": "^1",
        "amphp/socket": "^0.10.7",
        "league/uri-parser": "^1.3",
        "league/uri-schemes": "^1.1",
        "psr/http-message": "^1",
        "psr/log": "^1",
        "cash/lrucache": "^1.0"
    },
    "require-dev": {
        "amphp/phpunit-util": "^1",
        "amphp/artax": "^3",
        "amphp/log": "^1",
        "friendsofphp/php-cs-fixer": "^2.3",
        "league/uri-components": "^1.7",
        "monolog/monolog": "^1.23",
        "phpunit/phpunit": "^6",
        "infection/infection": "^0.9.2"
    },
    "autoload": {
        "psr-4": {
            "Amp\\Http\\Server\\": "src"
        },
        "files": [
            "src/Middleware/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Http\\Server\\Test\\": "test"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.13"
        }
    },
    "scripts": {
        "test": "@php -dzend.assertions=1 -dassert.exception=1 ./vendor/bin/phpunit",
        "code-style": "@php ./vendor/bin/php-cs-fixer fix"
    }
}
