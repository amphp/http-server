{
    "name": "amphp/aerys",
    "homepage": "https://github.com/amphp/aerys",
    "description": "A non-blocking HTTP/Websocket server",
    "keywords": [
        "http",
        "websocket",
        "server",
        "async",
        "non-blocking",
        "aerys",
        "amp",
        "amphp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net",
            "role": "Creator / Lead Developer"
        },
        {
            "name": "Bob Weinand",
            "role": "Developer / API Coordinator"
        },
        {
            "name": "Niklas Keller",
            "role": "Quality Assurance / Developer"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
        "php": ">=7",
        "amphp/amp": "^2",
        "amphp/file": "^0.2",
        "amphp/byte-stream": "^1",
        "amphp/socket": "^0.10",
        "league/climate": "^3",
        "nikic/fast-route": "^1",
        "psr/log": "^1",
        "ocramius/package-versions": "^1.1"
    },
    "require-dev": {
        "amphp/phpunit-util": "^1",
        "amphp/artax": "^3",
        "friendsofphp/php-cs-fixer": "^2.3@dev",
        "http2jp/hpack-test-case": "^1",
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "Aerys\\": "lib/"
        },
        "files": [
            "lib/functions.php",
            "lib/constants.php",
            "lib/Internal/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aerys\\Test\\": "test/"
        }
    },
    "bin": ["bin/aerys"],
    "extra": {
        "branch-alias": {
            "dev-master": "0.6.0-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "http2jp/hpack-test-case",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/http2jp/hpack-test-case",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ]
}
